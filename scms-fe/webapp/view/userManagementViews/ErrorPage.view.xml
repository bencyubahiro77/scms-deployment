<mvc:View
    controllerName="scmsfe.controller.userManagementControllers.ErrorPage"
    xmlns:mvc="sap.ui.core.mvc"
    displayBlock="true"
    xmlns:m="sap.m"
    xmlns:core="sap.ui.core"
    height="100%"
>
    <m:VBox
        id="_IDGenPage"
        class="sapUiContentPadding"
    >
        <m:IllustratedMessage
            id="_IDGenIllustratedMessage1"
            title="{i18n>504}"
            description="{i18n>unableToFindMetaData}"
            illustrationType="sapIllus-PageNotFound"
            illustrationSize="Auto"
        >
            <m:additionalContent>
                <m:Button
                    id="_IDGenButton4"
                    text="{i18n>retry}"
                    press=".onRetryPress"
                />
            </m:additionalContent>
        </m:IllustratedMessage>
    </m:VBox>
</mvc:View>
